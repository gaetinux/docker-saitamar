;
; Zone DNS reverse beesafe.co
;
$TTL    604800
@       IN	SOA   dns.beesafe.co. root.dns.beesafe.co. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; Name servers
@       IN	NS    dns.beesafe.co.
dns     IN	A     192.168.1.151
; Zone records
151     IN	PTR   dns.beesafe.co.
